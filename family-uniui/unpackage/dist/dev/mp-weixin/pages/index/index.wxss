.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-image: linear-gradient(to bottom, #1bbcb6 15%, rgba(221, 221, 221, 0.5) 31%, #fff 100%);
}
.container .header {
  display: flex;
  width: 700rpx;
  margin-top: 100rpx;
  gap: 30rpx;
  margin-bottom: 20rpx;
}
.container .header .search {
  display: flex;
  align-items: center;
  width: 420rpx;
  height: 64rpx;
  background: #ffffff;
  border: 2px solid #ffffff;
  border-radius: 8rpx;
}
.container .header .search .img {
  width: 36rpx;
  height: 32rpx;
  margin-left: 18rpx;
}
.container .header .search .span {
  opacity: 0.45;
  font-size: 28rpx;
  color: #333333;
  letter-spacing: 0.88px;
  margin-left: 16rpx;
}
.container .header .message {
  display: flex;
  align-items: center;
}
.container .header .message .img {
  width: 48rpx;
  height: 48rpx;
}
.container .swiper-container {
  width: 710rpx;
  height: 330rpx;
  margin-bottom: 20rpx;
}
.container .swiper-container .img {
  border-radius: 12rpx;
  width: 100%;
  height: 100%;
}
.container .more {
  margin-bottom: 20rpx;
  margin-left: 570rpx;
  font-size: 28rpx;
  color: #333;
  letter-spacing: 1.7rpx;
  text-align: center;
  font-weight: bold;
}
.container .services {
  box-sizing: border-box;
  display: flex;
  width: 710rpx;
  flex-wrap: wrap;
  gap: 50rpx;
  padding-left: 50rpx;
  background-image: url(data:image/png;base64,);
  border-radius: 20rpx;
}
.container .services .service {
  display: flex;
  align-items: center;
  flex-direction: column;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}
.container .services .service .img {
  width: 92rpx;
  height: 92rpx;
}
.container .services .service .span {
  font-size: 28rpx;
  color: #333333;
  letter-spacing: 1.7rpx;
  text-align: center;
}
.container .segments {
  width: 710rpx;
}
.container .segments .more2 {
  display: flex;
  height: 60rpx;
  justify-content: space-between;
  margin-top: 38rpx;
}
.container .segments .more2 .left {
  font-size: 44rpx;
  color: #333;
  letter-spacing: 2.8rpx;
}
.container .segments .more2 .right {
  line-height: 60rpx;
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
}
.container .segments .content {
  width: 710rpx;
  margin-top: 28rpx;
  display: flex;
  flex-wrap: wrap;
  gap: 12rpx;
}
.container .segments .content .item {
  display: flex;
  width: 345rpx;
  min-height: 212rpx;
  background-color: #fff;
  border-radius: 22rpx;
  flex-direction: column;
}
.container .segments .content .item .img {
  width: 322rpx;
  height: 212rpx;
  margin-top: 8rpx;
  margin-left: 12rpx;
  border-radius: 22rpx;
}
.container .segments .content .item .title {
  display: -webkit-box;
  margin-left: 18rpx;
  font-size: 26rpx;
  color: #333333;
  letter-spacing: 3rpx;
  margin-top: 14rpx;
  margin-bottom: 22rpx;
  overflow: hidden;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}