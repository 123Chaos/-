.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-image: linear-gradient(to bottom, #1bbcb6 15%, rgba(221, 221, 221, 0.4) 31%);
}
.container .header {
  display: flex;
  width: 700rpx;
  margin-top: 100rpx;
  gap: 30rpx;
  margin-bottom: 20rpx;
}
.container .header .search {
  display: flex;
  align-items: center;
  width: 420rpx;
  height: 64rpx;
  background: #ffffff;
  border: 2px solid #ffffff;
  border-radius: 8rpx;
}
.container .header .search .img {
  width: 36rpx;
  height: 32rpx;
  margin-left: 18rpx;
}
.container .header .search .span {
  opacity: 0.45;
  font-size: 28rpx;
  color: #333333;
  letter-spacing: 0.88px;
  margin-left: 16rpx;
}
.container .header .message {
  display: flex;
  align-items: center;
}
.container .header .message .img {
  width: 48rpx;
  height: 48rpx;
}
.container .content {
  display: flex;
  width: 100%;
  height: 100vh;
  flex-direction: column;
  align-items: center;
}
.container .content .banner {
  position: relative;
  display: flex;
  width: 700rpx;
  height: 216rpx;
  border-radius: 16rpx;
  margin-top: 20rpx;
  background-image: url(data:image/png;base64,);
}
.container .content .banner .left {
  width: 158rpx;
  height: 158rpx;
  margin-left: 26rpx;
  margin-top: 30rpx;
  margin-right: 52rpx;
  border-radius: 158rpx;
  box-shadow: 0 4rpx 6rpx 0 rgba(22, 83, 80, 0.23);
  overflow: hidden;
}
.container .content .banner .left .img {
  width: 100%;
  height: 100%;
}
.container .content .banner .right {
  margin-top: 25rpx;
  display: flex;
  flex-direction: column;
}
.container .content .banner .right .name {
  color: #333;
  font-size: 32rpx;
  letter-spacing: 2rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}
.container .content .banner .right .detail {
  color: #333;
  opacity: 0.45;
  letter-spacing: 1.7rpx;
  font-size: 28rpx;
}
.container .content .banner .right .inhospital {
  font-size: 28rpx;
  color: #0070ff;
  letter-spacing: 1.7rpx;
  text-align: center;
  margin-top: 30rpx;
}
.container .content .banner .tag {
  width: 116rpx;
  height: 52rpx;
  position: absolute;
  right: 24rpx;
  top: 16rpx;
  background: #f8ffff;
  border: 2rpx solid #1bbcb6;
  border-radius: 26rpx;
  font-size: 28rpx;
  color: #1bbcb6;
  letter-spacing: 1.7rpx;
  text-align: center;
  line-height: 52rpx;
}
.container .content .info {
  box-sizing: border-box;
  width: 700rpx;
  background: url(data:image/png;base64,) no-repeat right top;
  background-color: #fff;
  border-radius: 20rpx;
  padding-bottom: 20rpx;
  margin-top: 22rpx;
}
.container .content .info .marker {
  color: #1bbcb6;
  margin-right: 13rpx;
  margin-left: 20rpx;
}
.container .content .info .title {
  color: #333;
  font-size: 40rpx;
  letter-spacing: 2.5rpx;
  font-weight: bold;
}
.container .content .info .info-content {
  display: flex;
  font-size: 26rpx;
  flex-direction: column;
  gap: 20rpx;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
  margin-left: 40rpx;
  color: #333;
  letter-spacing: 2rpx;
}
.container .content .info .history {
  margin-top: 36rpx;
}
.container .content .info .basic {
  margin-top: 28rpx;
}
.container .content .control {
  display: flex;
  box-sizing: border-box;
  width: 700rpx;
  height: 100rpx;
  background-color: #fff;
  border-radius: 16rpx;
  justify-content: center;
  align-items: center;
  margin-top: 68rpx;
}
.container .content .control .img {
  width: 56rpx;
  height: 56rpx;
  margin-left: 37rpx;
}
.container .content .control .op {
  color: #333;
  font-size: 40rpx;
  letter-spacing: 2.5rpx;
}